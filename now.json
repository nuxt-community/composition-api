{
  "builds": [
    {
      "src": "docs/package.json",
      "use": "@now/static-build"
    },
    {
      "src": "test/fixture/api/posts.js",
      "use": "@now/node"
    },
    {
      "src": "package.json",
      "use": "@now/static-build"
    },
    {
      "src": "example/package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    { "src": "/api/posts/.*", "dest": "/test/fixture/api/posts.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/docs/$1" }
  ]
}
